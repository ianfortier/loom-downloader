{
    "name": "ianfortier/loom-downloader",
    "description": "Loom Downloader is a simple PHP package to download videos from loom.com",
    "keywords": [
        "ianfortier",
        "loom-downloader",
        "loom",
        "video",
        "download",
        "laravel",
        "php"
    ],
    "homepage": "https://github.com/ianfortier/loom-downloader",
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Fortier",
            "email": "github@ianfortier.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.3",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "LoomDownloader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LoomDownloader\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}